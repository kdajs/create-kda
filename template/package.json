{
  "name": "{{name}}",
  "main": "./dist/app.js",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build:dev": "kda-build ts --sourceMap",
    "build:release": "kda-build ts",
    "check:type": "tsc",
    "check:format": "eslint ./src/**/*.ts",
    "check": "run-s check:type check:format",
    "run": "kda run test",
    "start": "run-s build:dev run",
    "release:test": "kda pm2 test --reload",
    "release:production": "kda pm2 production --reload"
  },
  "gitHooks": {
    "pre-commit": "npm run check"
  },
  "dependencies": {
    "dayjs": "^1.10.4",
    "fp-ts": "^2.10.5",
    "io-ts": "^2.2.16",
    "kda": "{{kdaVersion}}",
    "shortid": "^2.2.16",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.27.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "kda-build": "{{kdaBuildVersion}}",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.3.2",
    "yorkie": "^2.0.0"
  }
}
